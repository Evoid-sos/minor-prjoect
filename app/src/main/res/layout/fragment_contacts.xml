        android:id="@+id/addContactButton"
        android:layout_width="43dp"
        android:layout_height="49dp"
        android:layout_marginStart="165dp"
        android:layout_marginTop="50dp"
        android:layout_marginEnd="165dp"
        android:layout_marginBottom="120dp"
        android:src="@drawable/add"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contact5Contacts" />

    <ImageView
        android:id="@+id/callContact1"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:layout_marginEnd="50dp"
        android:src="@drawable/callbutton"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/contact1Contacts"
        app:layout_constraintEnd_toEndOf="@+id/contact1Contacts"
        app:layout_constraintTop_toTopOf="@+id/contact1Contacts" />

    <ImageView
        android:id="@+id/callContact2"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:layout_marginEnd="50dp"
        android:src="@drawable/callbutton"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/contact2Contacts"
