        android:layout_height="40dp"
        android:layout_marginStart="123dp"
        android:layout_marginTop="24dp"
        android:layout_marginEnd="123dp"
        android:background="@color/cardview_dark_background"
        android:gravity="center|left"
        android:textColor="@color/design_default_color_on_primary"
        android:visibility="invisible"
        app:layout_constraintBottom_toTopOf="@+id/addContactButton"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/contact4Contacts" />

    <ImageView
        android:id="@+id/deleteContact1"
        android:layout_width="31dp"
        android:layout_height="31dp"
        android:src="@drawable/remove_contact"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="@+id/contact1Contacts"
        app:layout_constraintEnd_toEndOf="@+id/contact1Contacts"
        app:layout_constraintTop_toTopOf="@+id/contact1Contacts" />

    <ImageView
        android:id="@+id/deleteContact2"
        android:layout_width="31dp"
